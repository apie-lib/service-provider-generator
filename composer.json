{
    "name": "apie/service-provider-generator",
    "description": "Creates a service provider for Laravel from a Symfony services.yaml",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Apie\\ServiceProviderGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Apie\\Tests\\ServiceProviderGenerator\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Pieter Jordaan",
            "email": "pieter_jordaan@hotmail.com"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "symfony/yaml": "6.*",
        "illuminate/support": "7.*|8.*|9.*|10.*",
        "symfony/dependency-injection": "6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "10.*",
        "symfony/finder": "6.*"
    }
}
